{
  "version": 2,
  "builds": [
    {
      "src": "*.csproj",
      "use": "@microsoft/dotnet.js",
      "config": {
        "dotnet": "6.0"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "WeAgentAutoReply.dll"
    }
  ],
  "headers": [
    {
      "source": "api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0, s-maxage=0"
        }
      ]
    }
  ]
}